{
  "name": "dep-track",
  "version": "0.0.1",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "scripts": {
    "dev": "npx tsx ./src/index.ts",
    "server:cjs": "node ./cjs/index.js",
    "server:esm": "node ./esm/index.js",
    "build": "yarn build:cjs & yarn build:esm",
    "build:cjs": "tsc --p ./cjs/tsconfig.json",
    "build:esm": "tsc --p ./esm/tsconfig.json",
    "build:orig": "tsc --outDir ./dist && cp -R public dist/public"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "express": "^4.21.1",
    "tsx": "^4.19.1",
    "typescript": "^4.9.5"
  }
}
